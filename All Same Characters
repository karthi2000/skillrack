#include<studio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
  char s[1000];
  scanf("%s",s);
  char a=s[0];
  int h=strlen(s);
  int count=0;
  for(int i=1;i<h;i++)
  {
    if(s[i]!=a)
    {
      count=count+1;
     }
   }
   if(count>0)
   {
    printf("NO");
    }
    else
    {
    printf("YES");
    }
   }
