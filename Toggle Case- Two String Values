#include<iostream>
using namespace std;
int main(int argc,char** argv)
{
  string ch1,s;
  cin>>ch1>>s;
  int n1=ch1.length();
  for(int i=0;i<n1;i++)
  {
    int m=int(ch1[i]);
    if(s[i]=='U'||s[i]=='u')
    {
      if(m>96)
      {
        m=m-32;
        cout<<char(m);
      }
      else
      {
        cout<<char(m);
      }
    }
    else
    {
      if(m<96)
      {
        m=m+32;
        cout<<char(m);
      }
      else
      {
        cout<<char(m);
       }
      }
    }
}
        
